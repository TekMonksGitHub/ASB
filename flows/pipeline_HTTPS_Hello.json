{
	"flow": {
		"name":"Hello world HTTPS API",
		"disabled": true,
        "expandRouteProperties": true
	},
	"listener": {
		"type":"http_listener", 
		"isMessageGenerator": true,
		"host": "127.0.0.1",
		"port": 443,
		"url":"/hello",
		"allow_origin": "*",
		"timeout": 120000,
		"ssl": true,
		"sslKeyFile": "example/path/keyFile",
    	"sslCertFile": "example/path/CertFile" 
	},
	"route0":{
		"type": "js",
		"dependencies":["listener"],
		"isAsync": false,
		"js":"message.content.response = `Hello ${message.content.name}`; delete message.content.name;"
	},
	"output": {
		"type":"http_responder",
		"dependencies":["listener", "route0"]
	}
}