{
	"flow":{
		"name":"Multiple CSVs to JSONs",
		"disabled":true,
		"expandRouteProperties": true
	},
	"listener": {
		"type":"file", 
		"isMessageGenerator": true,
		"path":"{{ESB_DIR}}/in/*^GSPC.csv",
		"donePath":"{{ESB_DIR}}/processing"
	},
	"route0":{
		"type": "csvfilereader",
		"dependencies":["listener"],
		"rowsPerParse": 10000,
		"skip_first_row":true,
        "donePath":"{{ESB_DIR}}/done"
	},
	"route1": {
		"type":"csvparser",
		"dependencies":["route0","!listener"],
		"csv_headers":["Date","Open","High","Low","Close","Adj Close","Volume"],
		"trimValues": true
	},
	"output": {
		"type": "filewriter",
		"dependencies":"route1",
		"interceptor_module":"{{ESB_DIR}}/custom/multipath_interceptor.js",
		"interceptor_js":"",
		"write_ndjson": true,
		"append": true,
		"writeCloseTimeout": 5000,
		"encoding": "utf8"
	}
}