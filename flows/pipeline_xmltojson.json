{
	"flow":{
		"name":"XML to JSON",
		"disabled":false
	},
	"listener": {
		"type":"file", 
		"isMessageGenerator": true,
		"path":"/home/dlt/Documents/ASB-master/in/*.xml",
		"donePath":"/home/dlt/Documents/ASB-master/processing"
	},
	"route0":{
		"type": "filereader",
		"dependencies":["listener"],
		"maxFileOpens":1000,
        "donePath":"/home/dlt/Documents/ASB-master/done",
        "encoding":"utf8"
	},
	"route1": {
		"type":"xmlparser",
		"dependencies":["route0"]
	},
	"output": {
		"type": "filewriter",
		"dependencies":"route1",
		"interceptor_module":"/home/dlt/Documents/ASB-master/custom/multipath_interceptor.js",
		"interceptor_js":"",
		"outputPath":"/home/dlt/Documents/ASBTEST/",
		"write_ndjson": true,
		"append": true,
		"writeCloseTimeout": 5000,
		"encoding": "utf8"
	}
}